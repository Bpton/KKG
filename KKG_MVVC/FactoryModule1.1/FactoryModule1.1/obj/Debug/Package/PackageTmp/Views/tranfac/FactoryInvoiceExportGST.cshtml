                    <div style="margin-top:10px;"></div>
                    <div class="col-xs-1">
                        Total&nbsp;Case:
                    </div>
                    <div class="col-xs-2">
                        @Html.TextBoxFor(model => model.TotalCase, new { @class = "form-control", @disabled = "disabled" })
                    </div>
                    <div class="col-xs-1">
                        Total&nbsp;PCS:
                    </div>
                    <div class="col-xs-2">
                        @Html.TextBoxFor(model => model.TotalPcs, new { @class = "form-control", @disabled = "disabled" })
                    </div>
                    <div class="col-xs-3">

                    </div>
                    <div class="col-xs-3">

                    </div>
                </div>

                <div class="row col-xs-12">
                    <div style="margin-top:10px;"></div>
                    <div class="col-xs-1">
                        Remarks:
                    </div>
                    <div class="col-xs-2">
                        @Html.TextAreaFor(model => model.Remarks, new { @class = "form-control", @rows = 4, @onkeypress = "return true;", style = "width: 187px;height: 95px;" })
                    </div>
                    <div class="col-xs-1">
                        Amt&nbsp;In&nbsp;Word(s)&nbsp;<span style="color:red"><b>*</b></span>&nbsp;:
                    </div>
