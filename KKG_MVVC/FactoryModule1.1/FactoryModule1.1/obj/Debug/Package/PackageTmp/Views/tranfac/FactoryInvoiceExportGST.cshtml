                    <div style="margin-top:10px;"></div>
                    <div class="col-xs-1">
                        Gross&nbsp;Amt.:
                    </div>
                    <div class="col-xs-2">
                        @Html.TextBoxFor(model => model.GrossAmt, new { @class = "form-control", @disabled = "disabled" })
                    </div>
                    <div class="col-xs-1">
                        Adjustment&nbsp;Amt.:
                    </div>
                    <div class="col-xs-2">
